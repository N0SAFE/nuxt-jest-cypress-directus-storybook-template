{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".nuxt/**", ".output/**", "storybook-static", "node_modules/.cache/**"]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "test": {
            "outputs": ["coverage/**", "cypress/screenshots/**", "cypress/videos/**", "jest/**", "test-results/**"],
            "dependsOn": ["build", "lint"],
            "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "cypress/e2e/**/*.cy.{js,jsx,ts,tsx}", "**/*.test.*"],
            "outputMode": "full"
        },
        "format": {
            "outputs": []
        },
        "typecheck": {
            "outputs": []
        },
        "docs": {
            "outputs": ["docs/**"]
        },
        "release": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "publish": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "clean": {
            "outputs": []
        }
    },
    "globalDependencies": [".env", "tsconfig.json"]
}
